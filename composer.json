{
    "name": "xsanisty/silex-starter",
    "description": "A starter application based on Silex framework",

    "repositories" : [
        {
            "type" : "vcs",
            "url" : "https://github.com/xsanisty/SilexStarter-Core"
        },
        {
            "type" : "vcs",
            "url" : "https://github.com/xsanisty/SilexStarter-Dashboard"
        }
    ],

    "require": {
        "xsanisty/silex-starter-core" : "dev-develop",
        "xsanisty/silex-starter-dashboard" : "dev-develop"
    },
    "require-dev": {
        "silex/web-profiler" : "1.*",
        "symfony/var-dumper" : "*"
    },
    "autoload": {
        "psr-4": {
            "" : [
                "src/",
                "app/controllers/",
                "app/models/",
                "app/middlewares/",
                "app/modules/",
                "app/repositories/",
                "app/services",
                "app/tests/app",
                "app/tests"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ikhsan Agustian",
            "email": "ikhsan017@gmail.com"
        }
    ],
    "minimum-stability": "stable"
}
